<tr>
  <td class="content_table_title" colspan="2">ユーザ登録</td>
</tr>
<tr>
  <th>
    <%= display_form_attr_name(User, "login") %>
  </th>
  <td>
    <%= h @user.login %>
    <%= f.hidden_field :login %>
    <%= hidden_field_tag :private_token, params[:private_token] %>
  </td>
</tr>
<tr>
  <th>
    [希望パスワード]<%= font_coution '※' %>
  </th>
  <td>
    <%= not_print_password(@user.password) %>
    <%= f.hidden_field :password %>
    <%= f.hidden_field :password_confirmation %>
  </td>
</tr>
